        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="100"/>
            </basic>
            <basic name="sort">
                <column name="sort" nullable="false" precision="11"/>
            </basic>
            <basic name="status">
                <column name="status" nullable="false" precision="1"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
