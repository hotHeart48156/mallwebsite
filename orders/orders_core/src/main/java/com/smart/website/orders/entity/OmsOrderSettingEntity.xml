            </basic>
            <basic name="normalOrderOvertime">
                <column name="normal_order_overtime" nullable="false" precision="11"/>
            </basic>
            <basic name="confirmOvertime">
                <column name="confirm_overtime" nullable="false" precision="11"/>
            </basic>
            <basic name="finishOvertime">
                <column name="finish_overtime" nullable="false" precision="11"/>
            </basic>
            <basic name="commentOvertime">
                <column name="comment_overtime" nullable="false" precision="11"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
