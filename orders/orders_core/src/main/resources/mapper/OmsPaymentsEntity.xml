<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.orders.entity.Payments">
        <table name="oms_payments" schema="orders" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="5"/>
            </id>
            <basic name="code">
                <column name="code" nullable="false" length="50"/>
            </basic>
            <basic name="name">
                <column name="name" nullable="false" length="50"/>
            </basic>
            <basic name="isOnline">
                <column name="is_online" nullable="false" precision="1"/>
            </basic>
            <basic name="params">
                <column name="params"/>
            </basic>
            <basic name="sort">
                <column name="sort" precision="5"/>
            </basic>
            <basic name="memo">
                <column name="memo" length="200"/>
            </basic>
            <basic name="status">
                <column name="status" precision="1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
