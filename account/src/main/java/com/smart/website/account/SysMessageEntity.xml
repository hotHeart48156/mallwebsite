<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.website.account.SysMessageEntity">
        <table name="sys_message" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="10"/>
            </id>
            <basic name="userId">
                <column name="user_id" precision="10"/>
            </basic>
            <basic name="code">
                <column name="code" length="60"/>
            </basic>
            <basic name="params">
                <column name="params" length="5000"/>
            </basic>
            <basic name="content">
                <column name="content"/>
            </basic>
            <basic name="ctime">
                <column name="ctime" precision="12"/>
            </basic>
            <basic name="utime">
                <column name="utime" precision="12"/>
            </basic>
            <basic name="status">
                <column name="status" precision="1"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
