<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.website.account.SysNoticeEntity">
        <table name="sys_notice" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="title">
                <column name="title" nullable="false" length="200"/>
            </basic>
            <basic name="content">
                <column name="content" nullable="false"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false" precision="1"/>
            </basic>
            <basic name="ctime">
                <column name="ctime" nullable="false" precision="12"/>
            </basic>
            <basic name="sort">
                <column name="sort" nullable="false" precision="3"/>
            </basic>
            <basic name="isdel">
                <column name="isdel" nullable="false" precision="12"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
