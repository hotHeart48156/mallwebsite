<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.account.entity.SysPermissionCategoryEntity">
        <table name="sys_permission_category" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="100"/>
            </basic>
            <basic name="icon">
                <column name="icon" nullable="false" length="500"/>
            </basic>
            <basic name="typeCount">
                <column name="type_count" nullable="false" precision="11"/>
            </basic>
            <basic name="showStatus">
                <column name="show_status" nullable="false" precision="2"/>
            </basic>
            <basic name="sort">
                <column name="sort" nullable="false" precision="11"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
