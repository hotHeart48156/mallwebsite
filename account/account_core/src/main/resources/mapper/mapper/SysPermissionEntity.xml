<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.account.entity.SysPermissionEntity">
        <table name="sys_permission" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="pid">
                <column name="pid" nullable="false" precision="20"/>
            </basic>
            <basic name="name">
                <column name="name" nullable="false" length="100"/>
            </basic>
            <basic name="value">
                <column name="value" nullable="false" length="200"/>
            </basic>
            <basic name="icon">
                <column name="icon" nullable="false" length="500"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false" precision="1"/>
            </basic>
            <basic name="uri">
                <column name="uri" nullable="false" length="200"/>
            </basic>
            <basic name="status">
                <column name="status" nullable="false" precision="1"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="sort">
                <column name="sort" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
