<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.account.entity.SysStorageEntity">
        <table name="sys_storage" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="realName">
                <column name="real_name" nullable="false"/>
            </basic>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="suffix">
                <column name="suffix" nullable="false"/>
            </basic>
            <basic name="path">
                <column name="path" nullable="false"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false"/>
            </basic>
            <basic name="size">
                <column name="size" nullable="false" length="100"/>
            </basic>
            <basic name="operate">
                <column name="operate" nullable="false"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
