<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.account.entity.SysQiniuConfigEntity">
        <table name="sys_qiniu_config" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="accessKey">
                <column name="access_key" nullable="false"/>
            </basic>
            <basic name="bucket">
                <column name="bucket" nullable="false"/>
            </basic>
            <basic name="host">
                <column name="host"/>
            </basic>
            <basic name="secretKey">
                <column name="secret_key" nullable="false"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false"/>
            </basic>
            <basic name="zone">
                <column name="zone" nullable="false"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
