<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.account.entity.SysUserEntity">
        <table name="sys_user" schema="account" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="username">
                <column name="username" nullable="false" length="64"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false" length="64"/>
            </basic>
            <basic name="icon">
                <column name="icon" nullable="false" length="500"/>
            </basic>
            <basic name="email">
                <column name="email" nullable="false" length="100"/>
            </basic>
            <basic name="nickName">
                <column name="nick_name" nullable="false" length="200"/>
            </basic>
            <basic name="note">
                <column name="note" nullable="false" length="500"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="loginTime">
                <column name="login_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="status">
                <column name="status" nullable="false" precision="1"/>
            </basic>
            <basic name="supplyId">
                <column name="supply_id" nullable="false" precision="20"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
