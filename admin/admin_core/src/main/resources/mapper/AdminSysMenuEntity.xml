<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.admin.entity.AdminSysMenuEntity">
        <table name="admin_sys_menu" schema="admin" catalog=""/>
        <attributes>
            <id name="menuId">
                <column name="menu_id" precision="20"/>
            </id>
            <basic name="menuName">
                <column name="menu_name" length="50"/>
            </basic>
            <basic name="parentId">
                <column name="parent_id" nullable="false" precision="20"/>
            </basic>
            <basic name="orderNum">
                <column name="order_num" nullable="false" precision="4"/>
            </basic>
            <basic name="url">
                <column name="url" nullable="false" length="200"/>
            </basic>
            <basic name="target">
                <column name="target" nullable="false" length="20"/>
            </basic>
            <basic name="menuType">
                <column name="menu_type" nullable="false" length="1"/>
            </basic>
            <basic name="visible">
                <column name="visible" nullable="false" length="1"/>
            </basic>
            <basic name="perms">
                <column name="perms" nullable="false" length="100"/>
            </basic>
            <basic name="icon">
                <column name="icon" nullable="false" length="100"/>
            </basic>
            <basic name="createBy">
                <column name="create_by" nullable="false" length="64"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="updateBy">
                <column name="update_by" nullable="false" length="64"/>
            </basic>
            <basic name="updateTime">
                <column name="update_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="remark">
                <column name="remark" nullable="false" length="500"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
