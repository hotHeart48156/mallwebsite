<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.admin.entity.AdminSysPostEntity">
        <table name="admin_sys_post" schema="admin" catalog=""/>
        <attributes>
            <id name="postId">
                <column name="post_id" precision="20"/>
            </id>
            <basic name="postCode">
                <column name="post_code" length="64"/>
            </basic>
            <basic name="postName">
                <column name="post_name" length="50"/>
            </basic>
            <basic name="postSort">
                <column name="post_sort" precision="4"/>
            </basic>
            <basic name="status">
                <column name="status" length="1"/>
            </basic>
            <basic name="createBy">
                <column name="create_by" nullable="false" length="64"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="updateBy">
                <column name="update_by" nullable="false" length="64"/>
            </basic>
            <basic name="updateTime">
                <column name="update_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="remark">
                <column name="remark" nullable="false" length="500"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
