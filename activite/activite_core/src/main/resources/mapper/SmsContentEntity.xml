<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.activite.entity.SmsContentEntity">
        <table name="sms_content" schema="activiti" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false"/>
            </basic>
            <basic name="meno">
                <column name="meno" nullable="false" length="44"/>
            </basic>
            <basic name="code">
                <column name="code" nullable="false"/>
            </basic>
            <basic name="remark">
                <column name="remark" nullable="false"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
