<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.activite.entity.SmsUserRedPacketEntity">
        <table name="sms_user_red_packet" schema="activiti" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="12"/>
            </id>
            <basic name="redPacketId">
                <column name="red_packet_id" precision="12"/>
            </basic>
            <basic name="adminId">
                <column name="admin_id" precision="12"/>
            </basic>
            <basic name="userId">
                <column name="user_id" nullable="false" precision="12"/>
            </basic>
            <basic name="amount">
                <column name="amount" precision="2"/>
            </basic>
            <basic name="grabTime">
                <column name="grab_time" precision="-1"/>
            </basic>
            <basic name="note">
                <column name="note" nullable="false" length="256"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
