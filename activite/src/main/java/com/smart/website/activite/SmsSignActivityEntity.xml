<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.website.activite.SmsSignActivityEntity">
        <table name="sms_sign_activity" schema="activiti" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="image">
                <column name="image" length="30"/>
            </basic>
            <basic name="starttime">
                <column name="starttime" length="20"/>
            </basic>
            <basic name="endtime">
                <column name="endtime" length="20"/>
            </basic>
            <basic name="detail">
                <column name="detail" nullable="false"/>
            </basic>
            <basic name="addTime">
                <column name="add_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="status">
                <column name="status" precision="4"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
