<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.website.activite.SmsRedPacketEntity">
        <table name="sms_red_packet" schema="activiti" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="12"/>
            </id>
            <basic name="userId">
                <column name="user_id" precision="12"/>
            </basic>
            <basic name="amount">
                <column name="amount" precision="2"/>
            </basic>
            <basic name="sendDate">
                <column name="send_date" nullable="false" precision="-1"/>
            </basic>
            <basic name="total">
                <column name="total" precision="12"/>
            </basic>
            <basic name="unitAmount">
                <column name="unit_amount" nullable="false" precision="12"/>
            </basic>
            <basic name="stock">
                <column name="stock" nullable="false" precision="12"/>
            </basic>
            <basic name="type">
                <column name="type" precision="12"/>
            </basic>
            <basic name="note">
                <column name="note" nullable="false" length="256"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
