                <column name="id" precision="20"/>
            </id>
            <basic name="memberId">
                <column name="member_id" nullable="false" precision="20"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="ip">
                <column name="ip" nullable="false" length="64"/>
            </basic>
            <basic name="city">
                <column name="city" nullable="false" length="64"/>
            </basic>
            <basic name="loginType">
                <column name="login_type" nullable="false" precision="1"/>
            </basic>
            <basic name="province">
                <column name="province" nullable="false" length="64"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
