<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.website.user.entity.UmsRewardLogsEntity">
        <table name="ums_reward_logs" schema="user" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="sendMemberId">
                <column name="send_member_id" nullable="false" precision="20"/>
            </basic>
            <basic name="recMemberId">
                <column name="rec_member_id" nullable="false" precision="20"/>
            </basic>
            <basic name="coin">
                <column name="coin" nullable="false"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
