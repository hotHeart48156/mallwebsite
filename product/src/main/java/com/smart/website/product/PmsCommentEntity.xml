<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.smart.website.product.PmsCommentEntity">
        <table name="pms_comment" schema="product1" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="productId">
                <column name="product_id" nullable="false" precision="20"/>
            </basic>
            <basic name="memberNickName">
                <column name="member_nick_name" nullable="false"/>
            </basic>
            <basic name="productName">
                <column name="product_name" nullable="false"/>
            </basic>
            <basic name="star">
                <column name="star" nullable="false" precision="3"/>
            </basic>
            <basic name="memberIp">
                <column name="member_ip" nullable="false" length="64"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="showStatus">
                <column name="show_status" nullable="false" precision="1"/>
            </basic>
            <basic name="productAttribute">
                <column name="product_attribute" nullable="false"/>
            </basic>
            <basic name="collectCouont">
                <column name="collect_couont" nullable="false" precision="11"/>
            </basic>
            <basic name="readCount">
                <column name="read_count" nullable="false" precision="11"/>
            </basic>
            <basic name="content">
                <column name="content" nullable="false"/>
            </basic>
            <basic name="pics">
                <column name="pics" nullable="false" length="1000"/>
            </basic>
            <basic name="memberIcon">
                <column name="member_icon" nullable="false"/>
            </basic>
            <basic name="replayCount">
                <column name="replay_count" nullable="false" precision="11"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
