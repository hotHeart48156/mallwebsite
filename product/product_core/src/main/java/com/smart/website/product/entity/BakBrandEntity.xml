<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.product.entity.BakBrandEntity">
        <table name="bak_brand" schema="product1" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="descs">
                <column name="descs"/>
            </basic>
            <basic name="picUrl">
                <column name="pic_url"/>
            </basic>
            <basic name="sortOrder">
                <column name="sort_order" nullable="false" precision="3"/>
            </basic>
            <basic name="floorPrice">
                <column name="floor_price" nullable="false" precision="2"/>
            </basic>
            <basic name="addTime">
                <column name="add_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="updateTime">
                <column name="update_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="deleted">
                <column name="deleted" nullable="false" precision="1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
