            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false" length="64"/>
            </basic>
            <basic name="pic">
                <column name="pic" nullable="false" length="200"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false" length="11"/>
            </basic>
            <basic name="sort">
                <column name="sort" nullable="false" precision="11"/>
            </basic>
            <basic name="description">
                <column name="description" nullable="false" length="1000"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
