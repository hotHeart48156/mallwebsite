<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.product_side.entity.CmsTopicCommentEntity">
        <table name="cms_topic_comment" schema="product_side" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="topicId">
                <column name="topic_id" nullable="false" precision="20"/>
            </basic>
            <basic name="memberNickName">
                <column name="member_nick_name" nullable="false"/>
            </basic>
            <basic name="memberIcon">
                <column name="member_icon" nullable="false"/>
            </basic>
            <basic name="content">
                <column name="content" nullable="false" length="1000"/>
            </basic>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="showStatus">
                <column name="show_status" nullable="false" precision="1"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
