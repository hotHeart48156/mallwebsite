<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <entity class="com.smart.website.product_side.entity.CmsPrefrenceAreaEntity">
        <table name="cms_prefrence_area" schema="product_side" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="subTitle">
                <column name="sub_title" nullable="false"/>
            </basic>
            <basic name="pic">
                <column name="pic" nullable="false" precision="500"/>
            </basic>
            <basic name="sort">
                <column name="sort" nullable="false" precision="11"/>
            </basic>
            <basic name="showStatus">
                <column name="show_status" nullable="false" precision="1"/>
            </basic>
            <basic name="storeId">
                <column name="store_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
